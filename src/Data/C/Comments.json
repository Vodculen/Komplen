{
	"singleLinedComments": [
		"#include <stdio.h>",
		"",
		"int main()",
		"{",
		"    // This prints out hello world to the screen",
		"    printf(\"Hello world\");",
		"",
		"    return 0;",
		"}"
	],

	"multilinedComments": [
		"#include <stdio.h>",
		"",
		"/* This function prints out \"Hello world!\"", 
		"It then returns 0 to appease the compiler */",
		"int main()",
		"{",
		"    printf(\"Hello world\");",
		"",
		"    return 0;",
		"}"
	]
}